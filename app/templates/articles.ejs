  <body>
    <%- include(process.cwd() + '/app/templates/navigation'); %>
    <main id="main-container">
      <ul>
        <li><a href="/">About <span class="symbol">↘</span></a></li>
        <li><a href="works.html">Works <span class="symbol">↘</span></a></li>
        <li class="selected">Articles</li>
        <ul id="articles-container" class="selected-container">
          <% articles.reverse().map((article) => { %>
          <li>
            <a href="./article/<%= article.id %>.html">
              <span class="heading"><%= article.title %><% if (article.subtitle) { %>:<% } %></span>
              <% if (article.subtitle) { %><span class="subheading"><%= article.subtitle %></span><% } %>
              <time>(<%= article.date %>)</time>
            </a>
          </li>
          <% }) %>
        </ul>
      </ul>
    </main>
    <%- include(process.cwd() + '/app/templates/footer'); %>
